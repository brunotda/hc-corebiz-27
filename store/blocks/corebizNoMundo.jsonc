{
  "flex-layout.row#noMundoContainer": {
    "props": {
      "fullWidth": true,
      "preventVerticalStretch": true,
      "blockClass": "noMundoContainerInicial"
    },
    "children": [
      "flex-layout.col#noMundoContainerGeral"
    ]
  },
  "flex-layout.col#noMundoContainerGeral": {
    "props": {
      "horizontalAlign": "CENTER",
      "verticalAlign": "MIDDLE",
      "width": "100%",
      "blockClass": "noMundoContainerGeral"
    },
    "children": [
      "rich-text#tituloNoMundo",
      "rich-text#subTituloNoMundo",
      "flex-layout.row#noMundoDados"
    ]
  },
  "rich-text#tituloNoMundo": { 
    "props": { 
      "text": "nossos números pelo mundo.",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-heading-1",
      "blockClass": "tituloNoMundo"
    }
  },
  "rich-text#subTituloNoMundo": { 
    "props": { 
      "text": "levamos um pouco da nossa visão pelo mundo",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "subTituloNoMundo"
    }
  },


  "flex-layout.row#noMundoDados": {
    "props": {
      "preventHorizontalStretch": true,
      "fullWidth": true,
      "blockClass": "noMundoDados"
    },
    "children": [
      "flex-layout.col#noMundoColab",
      "flex-layout.col#noMundopaises",
      "flex-layout.col#noMundoclientes"
    ]
  },

  "flex-layout.col#noMundoColab": {
    "props": {
      "blockClass": "noMundoColab"
    },
    "children": [
      "rich-text#tituloColab",
      "rich-text#textoColab"
    ]
  },
  "rich-text#tituloColab": { 
    "props": { 
      "text": "+340",
      "blockClass": "tituloDadosNoMundo"
    }
  },
  "rich-text#textoColab": { 
    "props": { 
      "text": "colaboradores pelo mundo",
      "blockClass": "textoDadosNoMundo"
    }
  },


  "flex-layout.col#noMundopaises": {
    "props": {
      "blockClass": "noMundopaises"
    },
    "children": [
      "rich-text#tituloPaises",
      "rich-text#textoPaises"
    ]
  },
  "rich-text#tituloPaises": { 
    "props": { 
      "text": "29 países",
      "blockClass": "tituloDadosNoMundo"
    }
  },
  "rich-text#textoPaises": { 
    "props": { 
      "text": "com clientes fixos ou projetos",
      "blockClass": "textoDadosNoMundo"
    }
  },

  "flex-layout.col#noMundoclientes": {
    "props": {
      "blockClass": "noMundoclientes"
    },
    "children": [
      "rich-text#tituloClientes",
      "rich-text#textoClientes"
    ]
  },
  "rich-text#tituloClientes": { 
    "props": { 
      "text": "+100",
      "blockClass": "tituloDadosNoMundo"
    }
  },
  "rich-text#textoClientes": { 
    "props": { 
      "text": "clientes ativos em diversos países",
      "blockClass": "textoDadosNoMundo"
    }
  }
}