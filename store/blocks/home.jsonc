{
  "store.home": { 
    "blocks": [
      "flex-layout.row#clientes",
      "slider-layout#caseclientes",
      "flex-layout.row#corebiz"
    ]
  },
  "flex-layout.row#corebiz": {
    "props":{
      "blockClass": "coreBiz",
      "fullWidth": true
    },
    "children":[
      "flex-layout.col#winner",
      "flex-layout.col#video"
    ]
  },
  "flex-layout.col#winner":{
    "props":{
      "blockClass": "winner"
    },
    "children":[
      "rich-text#somoscorebiz",
      "rich-text#corebiz",
      "rich-text#partners",
      "image-list#parceiros"
    ]
  },
  "rich-text#somoscorebiz": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "left",
      "text": "## somos a corebiz.",
      "textColor": "c-on-base--inverted",
      "font": "t-heading-5",
      "blockClass": "somoscorebiz"
    }
  },
  "rich-text#corebiz":{
    "props":{
      "textAlignment": "LEFT",
      "textPosition": "left",
      "text": "## a maior empresa de<br>experiência, tecnologia e<br>marketing para comércio<br>da América Latina.",
      "font": "t-heading-5",
      "blockClass": "corebiz"
    }
  },
  "rich-text#partners":{
    "props":{
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "nossos parceiros:",
      "textColor": "c-on-base--inverted",
      "font": "t-heading-5",
      "blockClass": "somoscorebiz"
    }
  },
  "image-list#parceiros":{
    "props": {
      "blockClass":"parceirosfotos",
      "images": [
        {
          "image": "https://www.corebiz.ag/wp-content/uploads/2020/06/vtex.png"
        },
        {
          "image": "https://www.corebiz.ag/wp-content/uploads/2020/06/salesforce.png"
        },
        {
          "image": "https://www.corebiz.ag/wp-content/uploads/2020/06/magento.png"
        }
      ]
    }
  },
  "flex-layout.col#video":{
    "props":{
      "blockClass": "video"
    },
    "children":[
     "modal-trigger#video"
    ]
  },
  "modal-trigger#video":{
    "children": [
      "modal-layout#video"
    ]
  },
  "modal-layout#video": {
    "children": [
      "video#corebiz"
    ]
  },
  "video#corebiz": {
    "props":{
      "src": "https://www.youtube.com/watch?v=Lep45Aztp7g",
      "autoPlay": true,
      "height": "420px",
      "width": "720px",
      "muted": "false"
    }
  },
  "slider-layout#caseclientes": {
    "children": [
      "info-card#case1",
      "info-card#case2",
      "info-card#case3",
      "info-card#case4"
      ],
    "props": {
      "itemsPerPage": {
        "desktop":1,
        "tablet":1,
        "phone": 1
        },
        "showPaginationDots": "never",
        "arrowSize": "10",
        "blockClass": "casesClientes"
      }
    },
  "info-card#case1": {
    "props": {
      "imageUrl": "https://www.corebiz.ag/wp-content/uploads/2020/06/case-slider-chillibeans-min.jpg",
      "headline": "case",
      "subhead": "5x mais receita no E-commerce da ChilliBeans",
      "callToActionText": "ver case completo",
      "textPosition": "left",
      "isFullModeStyle": true,
      "fullWidth": "100%"
    }
  },
  "info-card#case2": {
    "props": {
      "imageUrl": "https://www.corebiz.ag/wp-content/uploads/2020/06/case-slider-cc-min.jpg",
      "headline": "case",
      "subhead": "case elevação do faturamento de c&c",
      "callToActionText": "ver case completo",
      "textPosition": "left",
      "isFullModeStyle": true,
      "fullWidth": "100%"
    }
  },
  "info-card#case3": {
    "props": {
      "imageUrl": "https://www.corebiz.ag/wp-content/uploads/2020/06/case-slider-kitchenaid-min.jpg",
      "headline": "case",
      "subhead": "aumento do volume de vendas kitchenaid",
      "callToActionText": "ver case completo",
      "textPosition": "left",
      "isFullModeStyle": true,
      "fullWidth": "100%"
    }
  },
  "info-card#case4": {
    "props": {
      "imageUrl": "https://www.corebiz.ag/wp-content/uploads/2020/06/case-slider-black-friday-min.jpg",
      "headline": "case",
      "subhead": "maior ação de vendas do semparar na blackfriday",
      "callToActionText": "ver case completo",
      "link": "https://www.corebiz.ag/pt/cases/sem-parar/",
      "textPosition": "left",
      "isFullModeStyle": true,
      "fullWidth": "100%"
    }
  },
  "flex-layout.row#clientes": {
    "children": [
      "flex-layout.col#clientes"
    ],
    "props": {
      "horizontalAlign": "center",
      "fullWidth": true,
      "blockClass":"clientes"
    }
  },
  "flex-layout.col#clientes": {
    "children": [
      "rich-text#clientes",
      "flex-layout.row#clientesparceiros"
    ]
  },
  "rich-text#clientes": {
    "props": {
      "text": "## alguns de nossos<br> principais clientes",
      "blockClass": "principaisClientes"
    }
  },
  "flex-layout.row#clientesparceiros": {
    "children": [
      "image-list#clientesparceiros"
    ],
    "props": {
      "blockClass": "clientesParceiros",
      "fullWidth":true
    }
  },
