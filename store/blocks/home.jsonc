{
  "store.home": {
    "blocks": [
      "flex-layout.row#bannerHome",
      "flex-layout.row#clientes",
      "slider-layout#caseclientes",
      "flex-layout.row#corebiz"
    ]
  },
  "flex-layout.row#bannerHome": {
    "children": [
      "flex-layout.col#banner1",
      "flex-layout.col#banner2"
    ],
    "props": {
      "blockClass": "homeBanner"
    }
  },
  "media#banner": {
    "props": {
      "src": "assets/bannerhome.png",
      "blockClass": "bannerHomeImg"
    }
  },
  "flex-layout.col#banner1": {
    "children": [
      "flex-layout.row#bannerTextRow1",
      "flex-layout.row#bannerTextRow2",
      "flex-layout.row#bannerTextRow3"
    ],
    "props": {
      "horizontalAlign": "left",
      "blockClass": "bannerColLeft"
    }
  },
  "flex-layout.row#bannerTextRow1": {
    "children": [
      "rich-text#bannerRow1"
    ],
    "props": {
      "horizontalAlign": "LEFT"
    }
  },
  "rich-text#bannerRow1": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "**we are\nglobal.**",
      "textColor": "c-on-emphasis",
      "font": "t-heading-1",
      "blockClass": "slogan"
    }
  },
  "flex-layout.row#bannerTextRow2": {
    "children": [
      "rich-text#bannerRow2"
    ],
    "props": {
      "horizontalAlign": "LEFT"
    }
  },
  "rich-text#bannerRow2": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "levamos um pouco da nossa\nvisão pelo mundo",
      "textColor": "c-on-emphasis",
      "font": "t-heading-3",
      "blockClass": "bannerDescription"
    }
  },
  "flex-layout.row#bannerTextRow3": {
    "children": [
      "flex-layout.col#bannerTopic1",
      "flex-layout.col#bannerTopic2",
      "flex-layout.col#bannerTopic3"
    ],
    "props":{
      "blockClass": "infoDados"
    }
  },
  "flex-layout.col#bannerTopic1": {
    "children": [
      "flex-layout.row#bannerTopicTitle1",
      "flex-layout.row#bannerTopicText1"
    ],
    "props": {
      "blockClass": "topic1"
    }
  },
  "flex-layout.row#bannerTopicTitle1": {
    "children": [
      "rich-text#bannerTopicTitle1"
    ]
  },
  "flex-layout.row#bannerTopicText1": {
    "children": [
      "rich-text#bannerTopicText1"
    ]
  },
  "rich-text#bannerTopicTitle1": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "29 países",
      "textColor": "c-on-emphasis",
      "font": "t-heading-3",
      "blockClass": "topicTitle"
    }
  },
  "rich-text#bannerTopicText1": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "com clientes fixos\nou projetos",
      "textColor": "c-on-emphasis",
      "font": "t-heading-6",
      "blockClass": "topicText"
    }
  },
  "flex-layout.col#bannerTopic2": {
    "children": [
      "flex-layout.row#bannerTopicTitle2",
      "flex-layout.row#bannerTopicText2"
    ]
  },
  "flex-layout.row#bannerTopicTitle2": {
    "children": [
      "rich-text#bannerTopicTitle2"
    ]
  },
  "flex-layout.row#bannerTopicText2": {
    "children": [
      "rich-text#bannerTopicText2"
    ]
  },
  "rich-text#bannerTopicTitle2": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "+340",
      "textColor": "c-on-emphasis",
      "font": "t-heading-3",
      "blockClass": "topicTitle"
    }
  },
  "rich-text#bannerTopicText2": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "colaboradores pelo mundo",
      "textColor": "c-on-emphasis",
      "font": "t-heading-6",
      "blockClass": "topicText"
    }
  },
  "flex-layout.col#bannerTopic3": {
    "children": [
      "flex-layout.row#bannerTopicTitle3",
      "flex-layout.row#bannerTopicText3"
    ]
  },
  "flex-layout.row#bannerTopicTitle3": {
    "children": [
      "rich-text#bannerTopicTitle3"
    ]
  },
  "flex-layout.row#bannerTopicText3": {
    "children": [
      "rich-text#bannerTopicText3"
    ]
  },
  "rich-text#bannerTopicTitle3": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "+100",
      "textColor": "c-on-emphasis",
      "font": "t-heading-3",
      "blockClass": "topicTitle"
    }
  },
  "rich-text#bannerTopicText3": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "clientes ativos em diversos países",
      "textColor": "c-on-emphasis",
      "font": "t-heading-6",
      "blockClass": "topicText"
    }
  },
  "flex-layout.col#banner2": {},
  "flex-layout.row#corebiz": {
    "props": {
      "blockClass": "coreBiz",
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#winner",
      "flex-layout.col#video"
    ]
  },
  "flex-layout.col#winner": {
    "props": {
      "blockClass": "winner"
    },
    "children": [
      "rich-text#somoscorebiz",
      "rich-text#corebiz",
      "rich-text#partners",
      "image-list#parceiros"
    ]
  },
  "rich-text#somoscorebiz": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "## somos a corebiz.",
      "font": "t-heading-5",
      "blockClass": "somoscorebiz"
    }
  },
  "rich-text#corebiz": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "## a maior empresa de<br>experiência, tecnologia e<br>marketing para comércio<br>da América Latina.",
      "font": "t-heading-5",
      "blockClass": "corebiz"
    }
  },
  "rich-text#partners": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "nossos parceiros:",
      "font": "t-heading-5",
      "blockClass": "somoscorebiz"
    }
  },
  "image-list#parceiros":{
    "props": {
      "blockClass":"parceirosfotos",
      "images": [
        {
          "image": "https://www.corebiz.ag/wp-content/uploads/2020/06/vtex.png"
        },
        {
          "image": "https://www.corebiz.ag/wp-content/uploads/2020/06/salesforce.png"
        },
        {
          "image": "https://www.corebiz.ag/wp-content/uploads/2020/06/magento.png"
        }
      ]
    }
  },
  "flex-layout.col#video": {
    "props": {
      "blockClass": "video"
    },
    "children": [
      "modal-trigger#video"
    ]
  },
  "modal-trigger#video": {
    "children": [
      "modal-layout#video"
    ]
  },
  "modal-layout#video": {
    "children": [
      "video#corebiz"
    ]
  },
  "video#corebiz": {
    "props": {
      "src": "https://www.youtube.com/embed/Lep45Aztp7g",
      "description": "Video institucional Corebiz",
      "muted": false,
      "height": "420px",
      "width": "720px"
    }
  },
  "slider-layout#caseclientes": {
    "children": [
      "info-card#case1",
      "info-card#case2",
      "info-card#case3",
      "info-card#case4"
    ],
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "showPaginationDots": "never",
      "arrowSize": "10",
      "blockClass": "casesClientes"
    }
  },
  "info-card#case1": {
    "props": {
      "imageUrl": "https://www.corebiz.ag/wp-content/uploads/2020/06/case-slider-chillibeans-min.jpg",
      "headline": "case",
      "subhead": "5x mais receita no E-commerce da ChilliBeans",
      "callToActionText": "ver case completo",
      "textPosition": "left",
      "isFullModeStyle": true,
      "fullWidth": "100%"
    }
  },
  "info-card#case2": {
    "props": {
      "imageUrl": "https://www.corebiz.ag/wp-content/uploads/2020/06/case-slider-cc-min.jpg",
